{
  "plugins": [
    {
      "name": "FileExplorer",
      "description": "A file explorer plugin",
      "type": "org.headvances.ml.swingui.nlp.fexplorer.FileExplorerPlugin",
      "actionSet": {
        "popup": {
          "name": "RightClickPopup",
          "label": "Select An Action",
          "actions": [
            {
              "name": "Delete",
              "label": "Delete",
              "tooltip": "Delete this file or folfer",
              "description": "Delete this file or folder"
            }
          ]
        }
      }
    },
